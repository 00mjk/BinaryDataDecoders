<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="WriteDacVersion" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <Target Name="WriteDacVersion">
    <PropertyGroup>

      <DacVersion Condition="'$(DacVersion)'=='0.0.0.1'"></DacVersion>
      <Version Condition="'$(DacVersion)'==''"></Version>

      <DacVersion Condition="'$(DacVersion)'==''">0.0.0.0</DacVersion>
      <Version Condition="'$(Version)'==''">0.0.0-Local+0</Version>
    </PropertyGroup>
    <PropertyGroup>
      <DatabaseVersionScript>
-- Generated by MsBuild Step - DO NOT EDIT
DECLARE @DacVersion NVARCHAR(50)=&apos;$(DacVersion)&apos;;
DECLARE @BuildVersion NVARCHAR(50)=&apos;$(Version)&apos;;
      </DatabaseVersionScript>
      <SetDacVersionFile>$(ProjectDir)Scripts/PostDeployment/SetDacVersion.sql</SetDacVersionFile>
    </PropertyGroup>
    <Message Text="Writing $(SetDacVersionFile)" />
    <Message Text="Versions: $(DacVersion) = $(Version)" />
    <WriteLinesToFile
        File="$(SetDacVersionFile)"
        Lines="$(DatabaseVersionScript)"
        Overwrite="true"
        Encoding="Unicode" />
  </Target>
</Project>