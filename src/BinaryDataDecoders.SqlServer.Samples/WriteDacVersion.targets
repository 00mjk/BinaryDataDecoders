<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="WriteDacVersion" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <Target Name="WriteDacVersion">
    <PropertyGroup>
      <DacVersion Condition="'$(DacVersion)'==''">0.0.0.1</DacVersion>
      <Version Condition="'$(Version)'==''">0.0.0-LocalDev+0</Version>
    </PropertyGroup>
    <PropertyGroup>
      <DatabaseVersionScript>
        -- Generated by MsBuild Step - DO NOT EDIT
        DECLARE @DacVersion NVARCHAR(50)=&apos;$(DacVersion)&apos;;
        DECLARE @BuildVersion NVARCHAR(50)=&apos;$(Version)&apos;;
      </DatabaseVersionScript>
    </PropertyGroup>
    <Message Text="Writing $(ProjectDir)Scripts\PostDeployment\SetDacVersion.sql" />
    <Message Text="Versions: $(DacVersion) = $(Version)" />
    <WriteLinesToFile
        File="$(ProjectDir)Scripts\PostDeployment\SetDacVersion.sql"
        Lines="$(DatabaseVersionScript)"
        Overwrite="true"
        Encoding="Unicode" />
  </Target>
</Project>